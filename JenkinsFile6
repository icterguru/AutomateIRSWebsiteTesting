pipeline {
    agent any
    stages{
        stage("Init"){
           steps {
               echo "Testing JenkinsFile 6..."
           
           } 
        }
		}
    triggers{
			echo "Triggering for JenkinsFile 6..."
            pollSCM('* * * * *')
        }
  
    stage("Build"){
        steps{
            echo "Building JenkinsFile 6..."
        }
    }  
    
    stage("Deploy"){
        steps{
            echo "JenkinsFile6 Code deployment"
        }
    }    
    }
